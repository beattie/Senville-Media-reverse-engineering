# Senville/Midea Device Configuration
# Copy this file to .env and fill in your actual values

# Device Network Settings
SENVILLE_IP=192.168.1.100
SENVILLE_DEVICE_ID=123456789012345
SENVILLE_MAC=XX:XX:XX:XX:XX:XX

# V3 Authentication (required for local control)
# Get these by running: ./venv/bin/midea-discover -a YOUR_EMAIL -p YOUR_PASSWORD
SENVILLE_TOKEN=YOUR_128_CHARACTER_TOKEN_HERE
SENVILLE_KEY=YOUR_64_CHARACTER_KEY_HERE

# Midea Cloud Account (for discovery and cloud control)
MIDEA_ACCOUNT=your@email.com
MIDEA_PASSWORD=your_password

# Network Interface for Packet Capture (optional)
# Find yours with: ip addr show
NETWORK_INTERFACE=eth0

# Phone IP Address for Packet Capture (optional)
# Find yours in phone WiFi settings
PHONE_IP=192.168.1.50
